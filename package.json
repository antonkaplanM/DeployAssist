{
  "name": "deploy-assist",
  "version": "1.0.0",
  "description": "A deployment assistant app with Node.js and modern UI",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "start:ts": "node dist/app.js",
    "dev": "npm run build:css && node app.js",
    "dev:ts": "npm run build && npm run start:ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "build:css": "npx tailwindcss -i ./src/input.css -o ./public/tailwind.css --minify",
    "watch:css": "npx tailwindcss -i ./src/input.css -o ./public/tailwind.css --watch",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:e2e": "playwright test",
    "test:ci": "jest --coverage --runInBand",
    "pw:install": "playwright install",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "audit:backfill": "node backfill-ps-audit.js",
    "audit:capture": "node capture-ps-changes.js",
    "audit:fix-names": "node update-audit-account-names.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "db-migrate": "^0.11.14",
    "db-migrate-pg": "^1.5.2",
    "dotenv": "^17.2.2",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "jsforce": "^3.10.8",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "opossum": "^8.1.4",
    "pg": "^8.16.3",
    "pg-pool": "^3.10.1",
    "redis": "^5.8.2",
    "uuid": "^13.0.0",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@playwright/test": "^1.47.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsforce": "^1.11.5",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.15.5",
    "@types/redis": "^4.0.10",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nock": "^13.5.4",
    "postcss": "^8.5.6",
    "supertest": "^6.3.4",
    "tailwindcss": "^3.4.18",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "nodejs",
    "express",
    "deploy-assist",
    "integrations"
  ],
  "author": "",
  "license": "MIT"
}
